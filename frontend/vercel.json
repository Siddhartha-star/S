{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  },
  "framework": "nextjs",
  "github": {
    "enabled": false
  },
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  }
}



